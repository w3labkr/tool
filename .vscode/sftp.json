{
  "name": "name",
  "host": "hostname",
  "protocol": "ftp",
  "port": 21,
  "username": "username",
  "password": "password",
  "remotePath": "/",
  "downloadOnOpen": false,
  "uploadOnSave": true,
  "syncOption": {
    "update": true,
    "delete": true
  },
  "ignore": [
    "**/.DS_Store",
    "**/.svn",
    "**/.git/**",
    "**/node_modules/**",

    "**/.*rc",
    "**/.*rc.json",
    "**/.*ignore",
    "**/csscomb.json",

    "**/.vscode/**",
    "**/.history/**",

    "**/*.sublime-workspace",
    "**/*.sublime-project",
    "**/sftp-config.json",
    "**/sftp-config-alt*.json",
    "**/sftp-settings.json"
  ]
}
